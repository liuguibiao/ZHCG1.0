@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>子流程节点编辑</title>
</head>
<body>
    <!-- Modal -->
    <div class="modal-body">
        <div>
            <p>
                <span class="a_tit">子流程GUID:</span>
                <span><input type="text" id="txtProcessGUID" class="form-control" style="width:340px;" readonly="readonly"/></span>
                <span class="a_tit">子流程名称:</span>
                <span><input type="text" id="txtProcessName" class="form-control" style="width:340px;" readonly/></span>
            </p>
        </div>
        <div id="mySubProcessGrid" class="ag-bootstrap" style="width:100%;height:300px;float:left;"></div>
    </div>
    <div class="modal-footer">
        <button class="btn btn-primary" data-dismiss="modal" onclick="subprocessmanager.saveSubProcess();">确定</button>
        <button class="btn" data-dismiss="modal" aria-hidden="true">取消</button>
    </div>

    <script type="text/javascript">
        $(function () {
            subprocessmanager.load();
        })
    </script>
</body>
</html>